
@{
    ViewBag.Title = "Index";
}

<h2>Pracownicy</h2>

<br />
<table class="table table-bordered table-striped">
    <tr>
        <th>Imię</th>
        <th>Nazwisko</th>
        <th></th>
    </tr>
    @for (int i = 0; i < Model.Rows.Count; i++)
    {
        <tr>
            <td>@Model.Rows[i][0]</td>
            <td>@Model.Rows[i][1]</td>
            <td>
                <a href="@Url.Action("Edit", "Opinie", new { @id=Model.Rows[i][2] })">Dodaj</a> / 
                <a href="@Url.Action("Details", "Opinie", new { @id=Model.Rows[i][2] })">Zobacz opinie</a>
            </td>
        </tr>
    }
</table>

