@model System.Data.DataTable
@{
    ViewBag.Title = "Index";
}

<br />
<table class="table table-bordered table-striped">
    <tr>
        <th>Marka</th>
        <th>Model</th>
        <th>Data produkcji</th>
        <th>Pojemnosc</th>
        <th>Przebieg</th>
        <th>Przeglad</th>
        <th></th>
    </tr>
    @for(int i=0; i<Model.Rows.Count; i++)
    {
        <tr>
            <td>@Model.Rows[i][1]</td>
            <td>@Model.Rows[i][2]</td>
            <td>@Model.Rows[i][3]</td>
            <td>@Model.Rows[i][4]</td>
            <td>@Model.Rows[i][5]</td>
            <td>@Model.Rows[i][6]</td>
            <td>
                <a href="@Url.Action("Edit", "Samochod", new { @id=Model.Rows[i][0] })">Edycja</a> /
                <a href="@Url.Action("Delete", "Samochod", new { @id=Model.Rows[i][0] })">Usuń</a>
            </td>
        </tr>
    }
</table>

@*nazwa, nazwa kontrolera*@
<a href="@Url.Action("Create", "Samochod")">Dodaj samochód</a>

